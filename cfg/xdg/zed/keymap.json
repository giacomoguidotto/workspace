[
  {
    "context": "Workspace",
      "bindings": {
      "cmd-shift-j": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "(Editor && edit_prediction_conflict)",
    "bindings": {
      "alt-;": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "(Editor && edit_prediction)",
    "bindings": {
      "alt-;": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-space": "editor::ShowCompletions"
    }
  },
  {
    "bindings": {
      "cmd-o": [
        "projects::OpenRecent",
        {
          "create_new_window": false
        }
      ]
    }
  },
  {
    "bindings": {
      "alt-cmd-o": "workspace::Open"
    }
  }
]
